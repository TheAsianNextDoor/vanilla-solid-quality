{
  "name": "solid-quality",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "check-updates": "ncu",
    "clean": "rm -rf ./node_modules & rm ./package-lock.json",
    "coverage": "NODE_ENV=test node_modules/.bin/vitest run --coverage",
    "coverage:view": "NODE_ENV=test npm run coverage && open ./reports/index.html",
    "dev": "vite",
    "lint": "eslint './src'",
    "lint:fix": "concurrently -n prettier,eslint \"prettier --write . '!**/*.{js,jsx,ts,tsx}'\" \"eslint --fix .\"",
    "playwright:test": "npx playwright test --project=chromium --project=firefox",
    "playwright:debug-test": "npx playwright test --project=chromium --debug --",
    "preview": "vite preview",
    "start": "vite",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest watch",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@suid/vite-plugin": "^0.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7.1.20",
    "@types/eslint": "^8.4.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitest/ui": "^0.25.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.8.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.48.1",
    "node-fetch": "^3.3.0",
    "npm-check-updates": "^16.3.22",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "solid-testing-library": "^0.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-solid": "^2.4.0",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "@suid/icons-material": "^0.4.1",
    "@suid/material": "^0.6.3",
    "solid-js": "^1.6.2",
    "zod": "^3.19.1"
  }
}
